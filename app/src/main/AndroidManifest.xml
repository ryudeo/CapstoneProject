<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ryudeo.capstoneproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.WeightBriefActivity" />
        <activity android:name=".Activities.WaterBriefActivity" />
        <activity android:name=".Activities.ExerciseBriefActivity" />
        <activity android:name=".Activities.FoodBriefActivity" />
        <activity android:name=".Activities.FabWeightActivity" />
        <activity android:name=".Activities.FabWaterActivity" />
        <activity android:name=".Activities.FabExerciseActivity" />
        <activity android:name=".Activities.FabFoodActivity" />
        <activity android:name=".Activities.FabProfileActivity"></activity>
    </application>

</manifest>